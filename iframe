    <h1>Ejemplo de Botón con DataLayer</h1>
    <button onclick="enviarDataLayer()">Enviar Evento</button>

    <script>
        function enviarDataLayer() {
            dataLayer.push({
                "event": "eventTracker",
                "eventCat": "Blog", 
                "eventAct": "Buscador - Con resultado",
                "eventLbl": "Punta cana"
            });
        }
    </script>
    
    
  
